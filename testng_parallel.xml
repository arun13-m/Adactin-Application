<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite parallel="classes" name="Suite">

<listeners>
<listener class-name="utility.Listener"></listener>
</listeners>

  <test thread-count="5" parallel="classes" name="Chrome Test">
  <parameter name="browser" value="chrome"></parameter>
    <classes>
      <class name="test.TC_001_Register"/>
      <class name="test.TC_002_Login"/>
      <class name="test.TC_005_BookAHotel"/>
      <class name="test.TC_009_EndLogout"/>
      <class name="test.TC_003_SearchHotel"/>
      <class name="test.TC_006_BookingConfirmation"/>
      <class name="test.TC_008_ChangePassword"/>
      <class name="test.TC_004_SelectHotel"/>
      <class name="test.TC_007_BookedItinerary"/>
    </classes>
  </test> <!-- Test -->
  
  <test thread-count="5" parallel="classes" name="Edge Test">
  <parameter name="browser" value="edge"></parameter>
    <classes>
      <class name="test.TC_001_Register"/>
      <class name="test.TC_002_Login"/>
      <class name="test.TC_005_BookAHotel"/>
      <class name="test.TC_009_EndLogout"/>
      <class name="test.TC_003_SearchHotel"/>
      <class name="test.TC_006_BookingConfirmation"/>
      <class name="test.TC_008_ChangePassword"/>
      <class name="test.TC_004_SelectHotel"/>
      <class name="test.TC_007_BookedItinerary"/>
    </classes>
  </test> --><!-- Test --> 
  
</suite> <!-- Suite -->
